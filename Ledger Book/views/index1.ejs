<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title> Product Report</title>
    <link rel="stylesheet" href=" /stylessheets/index1.css">
</head>
<body>
    <table>
        <caption> Buying Product Report</caption>
        <thead>
            <tr>
                <th>Invoice No.</th>
                <th> Item </th>
                <th>Qty</th>
                <th>Rate</th>
                <th >Amount</th>
            </tr>
        </thead>
        <% users.forEach(function (user){%> 
        <tbody > 
            <tr id="each">
                
                <td > <%= user.invoice %></td>
                <td> <%= user.item %></td>
                <td id="qty3"> <%= user.qty %></td>
                <td id="rate3"> <%= user.rate%></td>
                <td id="amount3"> <%= user.total%></td>
                
                
            </tr>
          
        </tbody>
        <% })%>

  
        
        
        <tfoot>
            <tr>
                <td><strong>Total</strong></td>
                <td>650</td>
                <td>40</td>
                <td>$5,400</td>
                <td>$1,800</td>
            </tr>
        </tfoot>
    </table>








    <script src=" /javasripts/index1.js"></script>
</body>
</html>
